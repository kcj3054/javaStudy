package 배열실습2;



import java.util.Random;

public class lotto {

    public static void main(String[] args){
    
    	int [] number = new int[7];
    	
    	for	(int i = 0; i<number.length ; i++) {
    		
    		number[i] = (int)(Math.random() * 45)+1;
    		
    		for(int j = 0; j< i - 1; j++) {
    			
    			while(number[i] == number[j]) {
    				number[j] = (int)(Math.random() * 45) +1;
    				
    			}
    		}
    	}
    	
    }
    
    
}